        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "They both decided to kill themselves fighting.(13)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਦੋਹਾ ਨੇ ਲੜਾਈ ਵਿਚ ਜੂਝ ਮਰਨ ਦਾ ਮਤਾ ਪਕਾਇਆ ॥੧੩॥",
              "additional_information": {}
            }
          }
        }
      },
