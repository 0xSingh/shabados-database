      {
        "id": "G2RM",
        "source_page": 981,
        "source_line": null,
        "gurmukhi": "mqo bYiT duhUMAn ikXo; jUiJ mrn ky Bwie ]13]",
        "transliteration_english": "mato baitt dhuhoonan kiyo; joojh maran ke bhaae ||13||",
