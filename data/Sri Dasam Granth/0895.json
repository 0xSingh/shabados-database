          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਲਾਹੌਰ ਵਿਚ ਇਕ ਸੁਨਿਆਰਾ ਰਹਿੰਦਾ ਸੀ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "JP9D",
        "source_page": 895,
        "source_line": null,
        "gurmukhi": "Aiq qskr qw ko jg khY ]",
        "transliteration_english": "at tasakar taa ko jag kahai ||",
        "transliteration_hindi": "अति तसकर ता को जग कहै ॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Whom people used to know as a big swindler.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨੂੰ ਸਾਰੇ ਵੱਡਾ ਚੋਰ ਕਹਿੰਦੇ ਸਨ।",
              "additional_information": {}
