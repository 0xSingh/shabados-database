        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Considering him as the Raja of a country she (decided) to marry her daughter to him.(5)",
              "additional_information": {}
