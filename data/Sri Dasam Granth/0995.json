        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "129th Parable of Auspicious Chritars Conversation of the Raja and the Minister, Completed With Benediction. (129)(2561)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਥੇ ਸ੍ਰੀ ਚਰਿਤ੍ਰੋਪਾਖਿਆਨ ਦੇ ਤ੍ਰਿਯਾ ਚਰਿਤ੍ਰ ਦੇ ਮੰਤ੍ਰੀ ਭੂਪ ਦੇ ਸੰਵਾਦ ਦੇ ੧੨੯ਵੇਾਂ ਚਰਿਤ੍ਰ ਦੀ ਸਮਾਪਤੀ, ਸਭ ਸ਼ੁਭ ਹੈ ॥੧੨੯॥੨੫੬੩॥ ਚਲਦਾ॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
