        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "At first she showered her arrows and then her lion roared heavily.32.188.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਪਹਿਲਾ (ਬਰਖਾ ਦੀਆਂ) ਬੂੰਦਾ ਵਾਗ ਬਾਣਾ (ਦੀ ਵਾਛੜ ਲਾਈ) ਅਤੇ ਫਿਰ ਸਿੰਘ-ਨਾਦ ਕੀਤਾ ॥੩੨॥੧੮੮॥",
              "additional_information": {}
            }
          }
        }
