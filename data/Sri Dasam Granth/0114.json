        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "In this way, destroying the tyrants, Durga again wore her weapons and armour.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਸ ਤਰ੍ਰਹਾ ਦੁਸ਼ਟਾ ਨੂੰ ਨਸ਼ਟ ਕਰ ਕੇ (ਦੇਵੀ ਨੇ) ਹੱਥ ਵਿਚ ਅਸਤ੍ਰ ਅਤੇ ਸ਼ਸਤ੍ਰ ਫੜ ਲਏ।",
              "additional_information": {}
