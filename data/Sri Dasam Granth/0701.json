          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸੱਤਵੇਾਂ ਨਾ ਖੰਡਿਤ ਹੋਣ ਵਾਲੇ, ਅਪਾਰ ਅਤੇ ਅਤਿਅੰਤ ਪ੍ਰਤਾਪ ਵਾਲੇ ਯੋਧੇ ਨੂੰ",
              "additional_information": {}
            }
          }
