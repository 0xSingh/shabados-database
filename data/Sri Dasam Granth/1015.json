        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Balbhadar, and Anuradh and Krishna, the sons ofParduman.(21)",
              "additional_information": {}
            }
