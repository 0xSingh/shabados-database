          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰੋਹਣੀ ਦੇ ਪਤੀ (ਬਲਭਦ੍ਰ) ਪ੍ਰਦੁਮਨ ਦੇ ਪੁੱਤਰ ਅਨਰੁਧ ਅਤੇ ਕ੍ਰਿਸ਼ਨ ਆਦਿ ਯਾਦਵਾ ਨੂੰ ਪ੍ਰਸਤੁਤ ਕੀਤਾ ॥੨੧॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "KRAY",
        "source_page": 1015,
        "source_line": null,
        "gurmukhi": "cOdh purI bnwie kY; qwih kihXo smJwih ]",
        "transliteration_english": "choudheh puree banaae kai; taeh kahiyo samajhaeh ||",
        "transliteration_hindi": "चौदह पुरी बनाइ कै ताहि कहियो समझाहि ॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "After creating there, fourteen fairies, she suggested her,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚੌਦਾ ਪੁਰੀਆਂ ਬਣਾ ਕੇ ਉਸ ਨੂੰ ਸਮਝਾ ਕੇ ਕਿਹਾ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "DQVX",
        "source_page": 1015,
        "source_line": null,
        "gurmukhi": "qumro ijXb aupwie mY; kIXo iblokhu Awie ]22]",
        "transliteration_english": "tumaro jiyab aupaae mai; keeyo bilokahu aae ||22||",
        "transliteration_hindi": "तुमरो जियब उपाइ मै कीयो बिलोकहु आइ ॥२२॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘I have devised the means for your survival, come and see yourself.’(22)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਤੇਰੇ ਜੀਣ ਦਾ ਉਪਾ ਮੈਂ ਬਣਾਇਆ ਹੈ, ਆ ਕੇ ਵੇਖ ॥੨੨॥",
              "additional_information": {}
            }
          }
        }
      }
    ]
