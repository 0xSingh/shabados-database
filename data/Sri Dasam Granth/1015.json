          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾ (ਆਪਣੀ) ਪਿਆਰੀ ਲਈ ਦੁਖੀ ਹੋ ਕੇ ਧਰਤੀ ਉਤੇ ਡਿਗ ਪਈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "U2SQ",
        "source_page": 1015,
        "source_line": null,
        "gurmukhi": "Xwih sKI, ijih ibiD sO; ipXih imlwieXY ]",
        "transliteration_english": "yaeh sakhee, jih bidh so; piyeh milaaeiyai ||",
        "transliteration_hindi": "याहि सखी जिहि बिधि सौ पियहि मिलाइयै ॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "(In her mind) ‘I must strive to make her meet her lover,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਸਖੀ! ਕਿਸੇ ਤਰ੍ਰਹਾ ਉਸ ਪ੍ਰੀਤਮ ਨਾਲ ਇਸ ਨੂੰ ਮਿਲਾ ਦੇਈਏ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "ZXFS",
        "source_page": 1015,
        "source_line": null,
        "gurmukhi": "hO jOn supinXY lihXo; vhY lY AwieXY ]19]",
        "transliteration_english": "ho joun supaniyai lahiyo; vahai lai aaeiyai ||19||",
        "transliteration_hindi": "हौ जौन सुपनियै लहियो वहै लै आइयै ॥१९॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘By bringing in the person she had seen in the dream.’(19)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਊਖ ਕਲਾ ਕਹਿਣ ਲਗੀ) ਜੋ ਸੁਪਨੇ ਵਿਚ ਵੇਖਿਆ ਹੈ, ਉਸ ਨੂੰ ਲਿਆ ਕੇ ਮਿਲਾ ਦਿਓ ॥੧੯॥",
              "additional_information": {}
            }
          }
        }
      }
