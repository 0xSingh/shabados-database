        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "There was once a great Raja and Raj Kala was his wife.",
              "additional_information": {}
            }
