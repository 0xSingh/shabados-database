          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨੂੰ ਸਚੀ ਵਰਗੀ ਸਮਝ ਕੇ ਇੰਦਰ ਵੀ ਬਹੁਤ ਰੀਝਿਆ ਰਹਿੰਦਾ ਸੀ ॥੧॥",
              "additional_information": {}
            }
          }
        }
