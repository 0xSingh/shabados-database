        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "That Rani, day in and day out, used to make love with a thief.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਹ ਰਾਣੀ ਇਕ ਚੋਰ ਨਾਲ ਦਿਨ ਰਾਤ ਰਮਣ ਕਰਦੀ ਸੀ।",
              "additional_information": {}
            }
          }
        }
