        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "There was none like her; even, the god Indra fancied her.(l)",
              "additional_information": {}
            }
