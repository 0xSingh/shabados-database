        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "This story has only been narrated in brief  on knowing this, kindly do not jeer at me.28.",
              "additional_information": {}
            }
