        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Never tell lies, and to achieve something, one should not become greedy.",
              "additional_information": {}
            }
