        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "For this work, the king Manu was highly venerated in the whole world.8.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਇਸ ਤਰ੍ਰਹਾ) ਮਨੁ ਰਾਜੇ ਦਾ ਉਜਲਾ ਯਸ਼ ਜਗਤ ਵਿਚ ਭਰਪੂਰ ਹੋ ਗਿਆ ॥੮॥",
              "additional_information": {}
            }
          }
        }
      },
