        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "For this work, the king Manu was MANU, the sixteenth incarnation in BACHITTAR BATAK.16.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਇਥੇ ਸ੍ਰੀ ਬਚਿਤ੍ਰ ਨਾਟਕ ਗ੍ਰੰਥ ਦਾ ਮਨੁ ਰਾਜਾ ਅਵਤਾਰ ਸੋਲਹਵਾ ਸਮਾਪਤ, ਸਭ ਸ਼ੁਭ ਹੈ ॥੧੬॥",
              "additional_information": {}
            }
          }
