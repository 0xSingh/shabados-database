        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "No sooner than the sage departed after cursing her, Vishnu reached his home:",
              "additional_information": {}
