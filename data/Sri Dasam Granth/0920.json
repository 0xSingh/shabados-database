          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਥੇ (ਰਾਜੇ ਦੇ) ਸਾਹਮਣੇ ੩੬੦ ਇਸਤਰੀਆਂ ਨਾਚ ਕਰਦੀਆਂ ਹੁੰਦੀਆਂ ਸਨ ॥੯॥",
              "additional_information": {}
            }
          }
