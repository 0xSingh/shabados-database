        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "He who has killed my father, that mighty warriors should come forward and wage war with me.”518.",
              "additional_information": {}
