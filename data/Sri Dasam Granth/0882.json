        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘A thief had come to kill me, but (he) hit my wife instead.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਚੋਰ ਮੈਨੂੰ ਮਾਰਨ ਲਗਿਆ ਸੀ, ਪਰ ਮੇਰੀ ਇਸਤਰੀ ਨੂੰ ਘਾਓ ਲਗ ਗਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "TZU1",
        "source_page": 882,
        "source_line": null,
        "gurmukhi": "kwiF BgOqI qurqu mY; Xw ko dXo su Gwie ]10]",
        "transliteration_english": "kaadd bhagoutee turat mai yaa ko dhayo su ghaae ||10||",
        "transliteration_hindi": "काढि भगौती तुरतु मै या को दयो सु घाइ ॥१०॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘Hurriedly I drew my sword out and killed him too.’(10)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮੈਂ ਤੁਰਤ ਤਲਵਾਰ ਕਢ ਕੇ ਉਸ ਨੂੰ ਮਾਰ ਦਿੱਤਾ ॥੧੦॥",
              "additional_information": {}
            }
          }
        }
      }
