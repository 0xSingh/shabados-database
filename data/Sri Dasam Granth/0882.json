          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਰਾਣੀ ਨੇ ਮਨ ਵਿਚ ਇਸ ਤਰ੍ਰਹਾ ਸਮਝਿਆ ਕਿ ਦਾਸੀ ਨਾਲ ਆ ਰਹੀ ਹੈ ॥੫॥",
              "additional_information": {}
            }
          }
        }
      }
