      {
        "id": "5PD1",
        "source_page": 1005,
        "source_line": null,
        "gurmukhi": "Ab lO sdn idlIs ky; idRgn ibloky nwih ]",
        "transliteration_english": "ab lo sadhan dhilees ke dhrigan biloke naeh ||",
        "transliteration_hindi": "अब लौ सदन दिलीस के द्रिगन बिलोके नाहि ॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘I have not seen the house ofthe Emperor of Delhi.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮੈਂ ਅਜ ਤਕ ਦਿੱਲੀ ਦੇ ਬਾਦਸ਼ਾਹ ਦਾ ਮਹੱਲ ਨਹੀਂ ਵੇਖਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "K7FF",
        "source_page": 1005,
        "source_line": null,
        "gurmukhi": "XhY hOs mn mY cuBI; sunu kwijn ky nwih ]13]",
        "transliteration_english": "yahai hous man mai chubhee sun kaajin ke naeh ||13||",
        "transliteration_hindi": "यहै हौस मन मै चुभी सुनु काजिन के नाहि ॥१३॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘It is my greatest aspiration that I may go there.’(13)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਹੇ ਕਾਜ਼ੀਆਂ ਦੇ ਸ਼ਿਰੋਮਣੀ! ਸੁਣੋ, ਇਹੀ ਇੱਛਾ ਮੇਰੇ ਮਨ ਵਿਚ ਚੁਭੀ ਪਈ ਹੈ ॥੧੩॥",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "N295",
        "source_page": 1005,
        "source_line": null,
        "gurmukhi": "dyv swQ kwjI kihXo; Xw ko Bvn idKwie ]",
        "transliteration_english": "dhev saath kaajee kahiyo yaa ko bhavan dhikhaae ||",
        "transliteration_hindi": "देव साथ काजी कहियो या को भवन दिखाइ ॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Quazi ordered the demon, ‘Take her there to show her the palace,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਾਜ਼ੀ ਨੇ ਦੇਓ ਨੂੰ ਕਿਹਾ। ਇਸ ਨੂੰ ਬਾਦਸ਼ਾਹ ਦਾ ਮਹੱਲ ਵਿਖਾ ਦਿਓ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "GTFY",
        "source_page": 1005,
        "source_line": null,
        "gurmukhi": "bhuro Kwt auTwie kY dIjhu hÎW phucwie ]14]",
        "transliteration_english": "bahuro khaaT auttaae kai dheejahu hayaa(n) pahuchaae ||14||",
        "transliteration_hindi": "बहुरो खाट उठाइ कै दीजहु ह्यां पहुचाइ ॥१४॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘And after that pick up her bed and bring her back here.’(14)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਫਿਰ ਮੰਜੀ ਨੂੰ ਚੁਕ ਕੇ ਇਥੇ ਪਹੁੰਚਾ ਦਿਓ ॥੧੪॥",
              "additional_information": {}
            }
          }
        }
      }
