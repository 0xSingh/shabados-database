        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "With the help of woofs he swam over and using the bamboos, he crossed over.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਝੰਡਿਆਂ (ਬਾਸਾ) ਦੇ ਸਹਾਰੇ ਉਥੋਂ ਬਚ ਗਿਆ।",
              "additional_information": {}
            }
