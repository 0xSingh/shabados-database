      {
        "id": "CMMT",
        "source_page": 897,
        "source_line": null,
        "gurmukhi": "boil swhu ko Dwm pTwXo ]",
        "transliteration_english": "bol saahu ko dhaam pattaayo ||",
        "transliteration_hindi": "बोलि साहु को धाम पठायो ॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "When the Rani had pondered over this, she called the Shah to her house.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "(ਤਾ) ਉਸ ਸ਼ਾਹ ਨੂੰ ਘਰ ਬੁਲਾ ਭੇਜਿਆ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "FUKD",
        "source_page": 897,
        "source_line": null,
        "gurmukhi": "Bwiq Bwiq Awsn iqh dIno ]",
        "transliteration_english": "bhaat bhaat aasan tih dheeno ||",
        "transliteration_hindi": "भाति भाति आसन तिह दीनो ॥",
