        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨਾਲ ਭਾਤ ਭਾਤ ਦੇ ਆਸਣ ਦਿੱਤੇ",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "ZZT8",
        "source_page": 897,
        "source_line": null,
        "gurmukhi": "aur Apny qy judw n kIno ]11]",
        "transliteration_english": "aur apane te judhaa na keeno ||11||",
        "transliteration_hindi": "उर अपने ते जुदा न कीनो ॥११॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "She provided him varied postures and did not let him go away. (11)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਆਪਣੇ ਹਿਰਦੇ ਨਾਲੋਂ ਵਖ ਨਾ ਕੀਤਾ ॥੧੧॥",
              "additional_information": {}
            }
          }
        }
      }
