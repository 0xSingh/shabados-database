          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜਿਸ ਕਰ ਕੇ ਸ਼ਿਵ ਨੇ ਬਹੁਤ ਦੁਖ ਪਾਇਆ।",
              "additional_information": {}
            }
          }
        }
