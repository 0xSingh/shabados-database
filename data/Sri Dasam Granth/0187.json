        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then Rudra meditated upon the Lord and restored the life of all other kings.",
              "additional_information": {}
