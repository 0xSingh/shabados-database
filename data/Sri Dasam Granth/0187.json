        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਫਿਰ ਦਕਸ਼ ਰਾਜੇ ਦੀਆਂ ਪੁੱਤਰੀਆਂ ਦੇ ਸਾਰੇ ਪਤੀ ਜੀਵਿਤ ਕਰ ਦਿੱਤੇ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "CTNU",
        "source_page": 187,
        "source_line": null,
        "gurmukhi": "kauqk inriK sMq iqRpqwey ]49]",
