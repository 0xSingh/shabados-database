          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਰਾਜੇ ਦਕਸ਼ ਨੂੰ ਫਿਰ ਜੀਵਿਤ ਕਰ ਕੇ ਉਠਾ ਦਿੱਤਾ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "ZX0G",
        "source_page": 187,
        "source_line": null,
        "gurmukhi": "rwj suqw piq skl jIXwey ]",
        "transliteration_english": "raaj sutaa pat sakal jeeyaae ||",
        "transliteration_hindi": "राज सुता पति सकल जीयाए ॥",
