          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤੁਸੀਂ ਵੱਡੇ ਤਪਸਵੀ ਤੇ ਬਲਵਾਨ ਹੋ।",
              "additional_information": {}
