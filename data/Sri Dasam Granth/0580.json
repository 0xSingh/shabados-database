      },
      {
        "id": "2U31",
        "source_page": 581,
        "source_line": null,
        "gurmukhi": "auTY Gtw swvx jnu AweI ]302]",
        "transliteration_english": "auttai ghaTaa saavaN jan aae'ee ||302||",
        "transliteration_hindi": "उठै घटा सावण जनु आई ॥३०२॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Great warriors fell martyrs in that battlefield and there was commotion in the country of Indra, the lances and arrows spread in the world like the rushing forth of the clouds of Sawan.302.",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮਾਨੋ ਸਾਵਣ ਦੀ ਘਟ ਛਾ ਗਈ ਹੋਵੇ ॥੩੦੨॥",
              "additional_information": {}
            }
          }
        }
