        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Then they heard celestial utterance, ‘Where have you people lost your wits,",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਕਿ ਹੇ ਨੌਕਰੋ! ਤੁਹਾਡੀ ਹੋਸ਼ ਕਿਥੇ ਗਈ ਹੈ।",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "AXVU",
        "source_page": 953,
        "source_line": null,
        "gurmukhi": "joDw bfo jUiJ jh jwvY ]",
        "transliteration_english": "jodhaa baddo joojh jeh jaavai ||",
        "transliteration_hindi": "जोधा बडो जूझि जह जावै ॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਜੇ ਕੋਈ ਵੱਡਾ ਸੂਰਮਾ ਮਾਰਿਆ ਜਾਵੇ,",
              "additional_information": {}
            }
          }
        }
      },
      {
        "id": "441Z",
        "source_page": 953,
        "source_line": null,
