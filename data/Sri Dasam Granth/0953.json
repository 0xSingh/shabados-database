          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਨੌਕਰਾ (ਸੇਵਕਾ) ਨੇ ਕੋਲ ਪਹੁੰਚ ਕੇ ਉਸ ਨੂੰ ਗਲੇ ਨਾਲ ਲਗਾ ਲਿਆ ॥੩੬॥",
              "additional_information": {}
            }
          }
        }
      }
