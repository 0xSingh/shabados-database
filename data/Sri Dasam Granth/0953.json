        "transliteration_english": "ran chhit te tin koun auchaavai ||38||",
        "transliteration_hindi": "रन छित ते तिन कौन उचावै ॥३८॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘When a brave person passes away in a battle, who takes his body away?(38)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਾ ਉਸ ਨੂੰ ਰਣ-ਭੂਮੀ ਵਿਚੋਂ ਕੌਣ ਚੁਕਦਾ ਹੈ? ॥੩੮॥",
              "additional_information": {}
            }
          }
        }
      }
