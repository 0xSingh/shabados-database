          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਮਾਨੋ (ਅਚਾਨਕ ਕੋਈ) ਧਨਵਾਨ ਨਿਰਧਨ ਹੋ ਗਿਆ ਹੋਵੇ।",
              "additional_information": {}
            }
          }
        }
