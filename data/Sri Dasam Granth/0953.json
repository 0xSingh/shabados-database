        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "‘And take home his clothes and inform the people there.’(39)",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਅਤੇ ਇਸ ਦਾ ਘੋੜਾ ਅਤੇ ਬਸਤ੍ਰ ਲੈ ਕੇ ਘਰ ਜਾਓ ਅਤੇ ਜਾ ਕੇ ਸਾਰਾ ਸੁਨੇਹਾ ਦਿਓ ॥੩੯॥",
              "additional_information": {}
            }
          }
        }
      },
      {
