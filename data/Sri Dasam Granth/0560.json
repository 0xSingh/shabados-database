        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "Even then, the people without intellect, will not shirk from the performance of bad karmas",
              "additional_information": {}
            }
          },
          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਤਦ ਵੀ ਮੰਨਣਗੇ ਨਹੀਂ, ਖੋਟੀ ਮਤ ਦਾ ਪ੍ਰਚਾਰ ਕਰਨਗੇ ਅਤੇ ਬੁੱਧੀ ਤੇ ਮਰਯਾਦਾ ਦੇ ਕੱਚੇ ਹੋਣਗੇ।",
              "additional_information": {}
            }
          }
        }
      },
