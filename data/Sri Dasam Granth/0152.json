        "source_page": 152,
        "source_line": null,
        "gurmukhi": "mhwrwj rwjn ky rwjw ]",
        "transliteration_english": "mahaaraaj raajan ke raajaa ||",
        "transliteration_hindi": "महाराज राजन के राजा ॥",
        "pronunciation": null,
        "pronunciation_information": null,
        "type": null,
        "translations": {
