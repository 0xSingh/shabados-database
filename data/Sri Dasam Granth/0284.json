        "pronunciation_information": null,
        "type": null,
        "translations": {
          "English": {
            "Dr. Surinder Singh Kohli": {
              "translation": "The warriors of both sides were killed and for two pehars (about six houts) the war continued",
              "additional_information": {}
            }
