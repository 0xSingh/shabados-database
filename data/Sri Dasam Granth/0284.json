          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਸਾਰੀ ਸੈਨਾ ਨੂੰ ਮਰਵਾ ਕੇ ਕਿੱਥੇ ਜਾ ਕੇ ਲੁਕੇ ਬੈਠੇ ਹੋ ?",
              "additional_information": {}
            }
