          "Punjabi": {
            "Dr. Rattan Singh Jaggi": {
              "translation": "ਉਸ ਨੂੰ ਵੇਖ ਕੇ ਰਾਣੀ ਮੋਹਿਤ ਹੋ ਗਈ ਅਤੇ ਬਿਰਹੋਂ ਦੇ ਬਾਣ ਨਾਲ ਵਿੰਨ੍ਰਹੀ ਗਈ ॥੩॥",
              "additional_information": {}
            }
          }
